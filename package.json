{
  "name": "todo",
  "version": "1.0.0",
  "description": "A ToDo small app to show how angular and webpack get together.",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run clean && webpack-dev-server --hot --inline --port 9080",
    "clean": "mkdir -p dist && rm -Rf dist/*",
    "prebuild": "npm run clean",
    "build": "IS_BUILD=true webpack --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.4.7",
    "angular-animate": "^1.4.7",
    "angular-aria": "^1.4.7",
    "angular-material": "^0.11.2",
    "angular-touch": "^1.4.7",
    "css-loader": "^0.19.0",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.1",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "json-loader": "^0.5.3",
    "node-sass": "^3.3.3",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  }
}
